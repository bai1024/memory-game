h1 
  margin: 50px auto;
  width: 240px;

#container 
  width: 900px;
  height:440px
  margin: auto;
  display: flex
  flex-wrap: wrap
  border: solid 1px #C2D3E7

.grid
  border: solid 1px #C2D3E7;
  box-sizing: border-box;
  position: relative
  img
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
  > .back-image
    z-index: 1000

