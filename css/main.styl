h1 
  margin: 50px auto;
  width: 240px;
  font-family: Angelina
  color: #B7DCB8
  font-size: 50px

#container 
  width: 900px;
  height:440px
  margin: auto;
  display: flex
  flex-wrap: wrap
  border: solid 1px #C2D3E7

.grid
  border: solid 1px #C2D3E7;
  box-sizing: border-box;
  position: relative
  img
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
  > .back-image
    opacity:1

.alert
  display: none
  color: #6b6b6b;
  opacity: 1;
  flex-direction: column;
  align-items: center;
  position: relative;
  width: 450px;
  min-height: 200px;
  top: -352px
  margin: auto
  background-color: #fff;
  border: 1px solid #fff;
  border-radius: 5px;
  z-index: 1000;
  transition: all 0.7s ease;
  font-family:Bad Script
  h2
    font-family: Bad script
    margin: 20px 0 5px 0
  .alert-content
    display: block;
    font-size: 25px
    margin: 0;
    padding: 20px 20px 0px 20px;
  .alert-btn-container
    display: block;
    position: relative;
    margin: 0 auto;
    padding: 30px 0 10px 0;
    button
      display: inline-block;
      margin: 10px 10px;
      padding: 10px 30px 10px 30px;
      border: 2px solid #6b6b6b;
      border-radius: 5px;
      color: #fff;
      font-size: 18px;
      font-weight: 400;
      outline: none;
      cursor: pointer;
      transition: all 0.25s ease;
    .confirm
      color: #91c8ff;
      border-color: #91c8ff;
      background-color: transparent;
section
  width:125px
  margin: 40px auto
#button
  color: #F08280
  border: dashed 5px #41403E;
  padding: 1rem 1rem;
  letter-spacing: 1px;
  font-size: 40px;
  background: transparent;
  font-family: 'Patrick Hand SC', cursive
  outline: none

  

